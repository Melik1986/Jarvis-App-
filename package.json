{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "axon",
  "main": "client/index.js",
  "version": "1.0.0",
  "scripts": {
    "expo:dev": "EXPO_PACKAGER_PROXY_URL=https://$REPLIT_DEV_DOMAIN REACT_NATIVE_PACKAGER_HOSTNAME=$REPLIT_DEV_DOMAIN EXPO_PUBLIC_DOMAIN=$REPLIT_DEV_DOMAIN:5000 npx expo start --localhost",
    "expo:dev:local": "npx expo start",
    "server:dev": "cross-env NODE_ENV=development tsx server/src/main.ts",
    "server:dev:local": "npm run server:dev",
    "server:nest:dev": "cross-env NODE_ENV=development tsx server/src/main.ts",
    "server:nest:build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 tsc -p server/tsconfig.json",
    "server:nest:prod": "NODE_ENV=production node server/dist/main.js",
    "expo:start:static:build": "npx expo start --no-dev --minify --localhost",
    "expo:static:build": "node scripts/build.js",
    "server:build": "esbuild server/src/main.ts --platform=node --packages=external --bundle --format=esm --outdir=server_dist",
    "server:prod": "NODE_ENV=production node server_dist/index.js",
    "db:push": "drizzle-kit push",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "jest",
    "test:server": "jest --testPathPatterns=server/",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npx expo lint"
    ]
  },
